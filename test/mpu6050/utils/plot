#! /usr/bin/gnuplot

set term qt 0
set title "MPU6050 Auswertung Gyroscope"
plot 'test.dat' using 1 title 'Yaw' with lines, 'test.dat' using 2 title 'Pitch' with lines, 'test.dat' using 3 title 'Roll' with lines

pause -1

